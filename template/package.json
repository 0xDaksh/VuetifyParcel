{
    "name": "{{ name }}",
    "version": "0.0.0",
    "description": "{{ description }}",
    "main": "src/App.js",
    "private": true,
    "scripts": {
        "dev": "parcel src/index.template.html -p 3000",
        "build": "parcel build src/index.template.html --public-url ./",
        "lint": "eslint src"
    },
    "dependencies": {
        "vue": "^2.5.13",
        "vuetify": "^0.17.6",
        "vue-router": "^3.0.1"{{#router}},
        "vue-router": "^3.0.1"{{/router}}
    },
    "devDependencies": {
        {{#lint}}
        "eslint": "^3.19.0",
        "babel-eslint":"*",
        {{#if_eq lintConfig "standard"}}
        "eslint-config-standard": "^10.2.1",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-standard": "^3.0.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^5.2.0",
        {{/if_eq}}
        {{#if_eq lintConfig "airbnb"}}
        "eslint-config-airbnb-base": "^11.3.0",
        "eslint-plugin-import": "^2.7.0",
        {{/if_eq}}
        {{/lint}}
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-2": "^6.24.1",
        "parcel-bundler": "^1.5.1",
        "parcel-plugin-vue": "^1.5.0",
        "stylus": "^0.54.5",
        "vue-template-compiler": "^2.5.13",
        "chalk": "^2.0.1"
    },
    "keywords": [
        "vuetify",
        "parcel",
        "stylus",
        "boilerplate",
        "template"
    ],
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "author": "{{ author }}",
    "license": "MIT"
}
